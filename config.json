{
  "server": {
    "name": "mcp-scheduler",
    "version": "0.1.0",
    "address": "localhost",
    "port": 8080,
    "transport": "stdio"
  },
  "database": {
    "path": "/home/ec2-user/scheduler-mcp/scheduler.db"
  },
  "logging": {
    "level": "INFO",
    "file": "/home/ec2-user/scheduler-mcp/mcp_scheduler.log"
  },
  "scheduler": {
    "check_interval": 5,
    "execution_timeout": 300
  },
  "ai": {
    "model": "gpt-4o",
    "use_aws_q_model": true
  }
}
